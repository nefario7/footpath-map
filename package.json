{
  "name": "footpath-map",
  "version": "1.0.0",
  "description": "Dashboard to map bad footpath locations in Bangalore from Twitter posts",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "build": "cd frontend && npm install && npm run build",
    "fetch": "node scripts/fetchTweets.js",
    "reprocess": "node scripts/reprocessData.js",
    "enhance": "node scripts/enhanceLocations.js"
  },
  "keywords": [
    "bangalore",
    "footpath",
    "map",
    "twitter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "pg": "^8.18.0",
    "twitter-api-v2": "^1.15.2"
  }
}